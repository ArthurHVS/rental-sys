<div style="width: 100%; height: 400px;" id="map" class="map"></div>
<script type="text/javascript">
    var map = new ol.Map({
        target: 'map',
        layers: [
            new ol.layer.Tile({
                source: new ol.source.OSM()
            })
        ],
        view: new ol.View({
            center: ol.proj.fromLonLat([{{ carro.geo.geometry.coordinates }}]),
        zoom: 12
    })
        });
</script>