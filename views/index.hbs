        <div class="row row-fluid justify-content-center">
            {{#each pool}}
            <div class="col col-lg-4 col-sm-10 justify-content-center">
                <div class="card text-light bg-secondary align-items-center py-0">
                    <div class="card-header w-100 float-left">
                        <div class="float-left"><img width="32" src="/images/svg/{{this.brand}}.svg"></img></div>
                        <div class="float-right">
                            <h4>{{this.model}}</h4>
                        </div>
                    </div>
                    <img class="card-img-top" src={{this.image}} alt="" />

                    <div class="card-body py-0 w-100 text-center">
                        <div class="container container-fluid">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th class="px-0" scope="col"><i class="fas fa-users"></i>Reviews</th>
                                        <th scope="col"><i class="fas fa-star"></i>Média</th>
                                    </tr>
                                    <tr>
                                        <td>{{#if
                                            this.aggregateRating.reviewCount}}{{this.aggregateRating.reviewCount}}{{else}}0{{/if}}
                                        </td>
                                        <td>{{#if
                                            this.aggregateRating.ratingValue}}{{this.aggregateRating.ratingValue}}{{else}}
                                            -
                                            {{/if}}
                                        </td>
                                    </tr>
                                </thead>
                            </table>


                        </div>
                        <a href="/car/{{this.slug}}" class="col col-12 btn btn-lg btn-dark">Detalhes</a>

                        <a class="col col-12 btn btn-lg btn-success" onclick="popup('{{this.model}}','{{this.brand}}')">Alugue <i class="fa fa-whatsapp" aria-hidden="true"></i></a>

                        </div>
                        <div class="w-100 card-footer text-white py-0">
                            Disponível
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>


